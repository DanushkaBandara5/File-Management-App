<AnchorPane prefHeight="279.0" prefWidth="502.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep10.app.Controller.CopySceneController">
   <children>
      <VBox alignment="CENTER" layoutX="20.0" layoutY="30.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Easy Copy Application" textAlignment="RIGHT">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Label>
            <HBox prefHeight="25.0" prefWidth="560.0" spacing="5.0">
               <children>
                  <TextField fx:id="txtSource" maxWidth="1.7976931348623157E308" promptText="Soure Destination" HBox.hgrow="ALWAYS" />
                  <Button fx:id="btnSource" mnemonicParsing="false" onAction="#btnSourceOnAction" prefHeight="24.0" prefWidth="71.0" text="Browse" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="560.0" spacing="5.0">
               <children>
                  <TextField fx:id="txtTarget" maxWidth="1.7976931348623157E308" promptText="Target Desitination" HBox.hgrow="ALWAYS" />
                  <Button fx:id="btnTarget" mnemonicParsing="false" onAction="#btnTargetOnAction" prefHeight="24.0" prefWidth="71.0" text="Browse" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox prefHeight="18.0" prefWidth="200.0" />
